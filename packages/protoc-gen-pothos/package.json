{
  "name": "protoc-gen-pothos",
  "version": "0.5.0-alpha.20",
  "description": "Generate DSL for Pothos GraphQL from Protocol Buffers IDL",
  "keywords": [
    "graphql",
    "grpc",
    "nexus",
    "protobuf",
    "typescript"
  ],
  "bin": {
    "protoc-gen-nexus": "./bin/protoc-gen-pothos"
  },
  "repository": "git@github.com:proto-graphql/proto-nexus.git",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "types": "./lib/index.d.ts",
  "private": false,
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@proto-graphql/codegen-core": "^0.5.0-alpha.20",
    "@proto-graphql/proto-descriptors": "^0.5.0-alpha.20",
    "@proto-graphql/protoc-plugin-helpers": "^0.5.0-alpha.20",
    "@types/change-case": "^2.3.1",
    "change-case": "^4.1.2"
  },
  "devDependencies": {
    "@proto-nexus/google-protobuf": "^0.5.0-alpha.20",
    "@proto-nexus/protobufjs": "^0.5.0-alpha.20",
    "@testapis/node": "^0.5.0-alpha.18",
    "@testapis/node-native": "^0.5.0-alpha.20",
    "@types/glob": "7.2.0",
    "glob": "8.0.1"
  },
  "peerDependencies": {
    "graphql": "^15.4.0"
  },
  "files": [
    "bin/",
    "include/",
    "lib/",
    "module/"
  ],
  "scripts": {
    "build": "tsc --build . && tsc --build tsconfig.module.json",
    "clean": "rimraf lib/ module/ && tsc --build . --clean && tsc --build tsconfig.module.json --clean",
    "prepublish": "yarn clean && yarn build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "TEST_TARGET=integration jest"
  }
}
