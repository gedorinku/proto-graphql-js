{
  "name": "@proto-graphql/e2e-protoc-gen-nexus",
  "version": "0.0.1",
  "description": "Generated codes with protobufjs",
  "private": true,
  "devDependencies": {
    "@testapis/node": "^0.5.0",
    "@testapis/node-native": "^0.5.1",
    "@testapis/ts-proto": "^0.5.1",
    "protoc-gen-nexus": "^0.5.1"
  },
  "scripts": {
    "clean": "rimraf **/__generated__",
    "gen:pb-nexus": "buf generate ../../@testapis/proto/src",
    "gen:gql-schema": "ts-node --transpile-only -e \"require('glob').sync('./**/schema.ts').forEach(require)\"",
    "test:typecheck": "ls -1 e2e/**/tsconfig.json | xargs -I {} yarn tsc --noEmit -p {}",
    "test:e2e": "yarn clean && yarn gen:pb-nexus && yarn gen:gql-schema && jest"
  }
}
