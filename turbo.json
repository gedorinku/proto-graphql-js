{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build", "^build:test"]
    },
    "build:test": {
      "dependsOn": ["^build", "^build:test"]
    },
    "test:e2e": {
      "dependsOn": ["^build", "^build:test"]
    }
  }
}
